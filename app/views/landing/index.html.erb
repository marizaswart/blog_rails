<%= link_to 'new article', '/articles/new' %>

<ul>
  <% @articles.each do |article| %>
      <li>
        <%= link_to article.title, article_path(article) %><br />
        <%= article.content %>
        <p>Written by <%= article.author%> at <%= article.created_at.strftime('%Y-%m-%d') %></p>
      </li>
  <% end %>
</ul>